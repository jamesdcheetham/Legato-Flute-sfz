<control>
	label_cc7=Volume [cc7]			set_cc7=96
	label_cc10=Pan [cc10]           set_cc10=64
	label_cc11=Expression [cc11]	set_cc11=127
	label_cc1=Air [cc1]				set_cc1=127
	label_cc3=Dynamics [cc3]		set_cc3=0
	label_cc72=Release [cc72]		set_cc72=16

